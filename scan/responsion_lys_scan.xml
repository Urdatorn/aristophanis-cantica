        <strophe type="antistrophe" responsion="lys03">
          <l n="335-337" metre="ia ia¨ III sp">[#Ἤ][κου]{σα} [γὰρ] [τυ]{φο}{γέ}[ρον][#τα][ς ἄν]{δρα}[ς ἔρ][ρειν], {στε}{λέ}[χη]{#φέ}[ρον]{τα}[ς ὥσ][περ] {βα}{λα}[νεύ][σον]{τας} </l>
          <l n="338" metre="ia ia¨" skip="True">εἰς πόλιν ὡς τριτάλαντα βάρος, </l>
          <l n="339" metre="ia ia¨">[#δει]{€νό}{€τα}{τ' ἀ}[πει][λοῦν]{τα}{ς ἐ}[πῶν] </l>
          <l n="340" metre="2 ia¨ ia ia^">[ὡς] {πυ}{ρὶ} [χρὴ] [τὰς] {μυ}{σα}[ρὰς] {#γυ}[#ναῖ]{κα}[ς ἀν]{θρα}[κεύ][ειν]. </l>
          <l n="341" metre="ia ia¨ ia¨ ia^">[#Ἅ][ς, ὦ] {θε}[ά], [μή] {πο}{τ' ἐ}[γὼ] [πιμ]{πρα}{μέ}[να]{ς ἴ}[δοι]{μι}, </l>
          <l n="342-343" metre="ia ia¨ ia¨ ia¨ ia^">[#ἀλ]{€λὰ} {€πο}{λέ}[μου] [καὶ] {μα}{νι}[ῶν] [ῥυ]{σα}{μέ}[να][ς Ἑλ]{λά}{δα} [καὶ] {πο}[λί][τας]· </l>
          <l n="344" metre="ia ia¨">{#ἐ}[φ' οἷσ]{πε}[ρ, ὦ] [χρυ]{σο}{λό}[φα] </l>
          <l n="345" metre="ia ia¨">{€πο}{€λι}[#οῦ]{χε}, [σὰ][ς ἔσ]{χο}{ν ἕ}[δρας]. </l>
          <l n="346" metre="" skip="True">Καί σε καλῶ ξύμμαχον, ὦ </l>
          <l n="347-348" metre="ia¨ ia¨">[Τρι]{το}{γέ}[νει]', [ἤν] {τι}{ς ἐ}[κεί][να]{ς ὑ}{πο}[πιμ][πρῇ]{σι}{ν ἀ}[νήρ], </l>
          <l n="349" metre="ia ia^">{#φέ}[ρει]{ν ὕ}[δωρ] {με}[θ' ἡ][μῶν]. </l>
        </strophe>
      </canticum>
      <canticum>
        <strophe type="strophe" responsion="lys04">
          <l n="476-477" metre="ia 3 ^ia">[#Ὦ] [Ζεῦ], {τί} {€πο}{€τε} [χρη]{σό}{€με}{€θα} [τοῖσ]{δε} [τοῖς] [κνω]{δά}[λοις]; </l>
          <l n="478" metre="4 ^ia">[Οὐ] {γὰ}{€ρ ἔ}{€τ' ἀ}[νεκ]{τὰ} {€τά}{€δε} [γ', ἀλ]{λὰ} {€βα}{€σα}[νισ]{τέ}[ον] </l>
          <l n="479" metre="anapaestic tripody">{τό}{δε} [σοι] {τὸ} {πά}[θος] {με}{τ' ἐ}[μοῦ], </l>
          <l n="480" metre="2 an">{ὅ} {τι} [βου]{λό}{με}[ναί] {πο}{τε} [τὴν] {Κρα}{να}[ὰν] </l>
          <l n="481-482" metre="2 an">{κα}{τέ}{λα}{βο}{ν, ἐ}{φ' ὅ} {τι} {τε} {με}{γα}{λό}{πε}{τρο}{ν, ἄ}{€βα}{€τον} </l>
          <l n="483" metre="anapaestic tripody">{ἀ}{κρό}{€πο}{€λι}{ν ἱ}{ε}[ρὸν] {τέ}{με}[νος]. </l>
        </strophe>
        <strophe type="antistrophe" responsion="lys04">
          <l n="541" metre="ia 3 ^ia">{#ἔ}[γω]{γε} {€γὰ}{€ρ ἂ}[ν οὔ]{πο}{€τε} {€κά}[μοι]{μ' ἂ}[ν ὀρ][χου]{μέ}[νη], </l>
          <l n="542" metre="4 ia^">[οὐ]{δὲ} {€γό}{€να}[τ' ἂν] {κό}{€πο}{€ς ἕ}[λοι] {με} {€κα}{€μα}[τή]{ρι}{ος}. </l>
          <l n="543" metre="anapaestic tripody">{Ἐ}{θέ}[λω] {δ' ἐ}{πὶ} [πᾶν] {ἰ}{έ}[ναι] </l>
          <l n="544" metre="2 an">{με}{τὰ} [τῶν]{δ' ἀ}{ρε}[τῆ]{ς ἕ}{νε}[χ', αἷ]{ς ἔ}{νι} {€φύ}{€σις}, </l>
          <l n="545-546" metre="2 an">{ἔ}{νι} {χά}{ρι}{ς, ἔ}{νι} {θρά}{σο}ς, {ἔ}{νι} {δὲ} {τὸ} {σο}{φό}{€ν, ἔ}{€νι} </l>
          <l n="547" metre="anapaestic tripody">{φι}{λό}{€πο}{€λι}{ς ἀ}{ρε}[τὴ] {φρό}{νι}[μος]. </l>
