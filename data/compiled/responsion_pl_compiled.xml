          <l n="299" metre="2 ia"><syll weight="heavy" anceps="True">ἡ</syll><syll weight="heavy">γού</syll><syll weight="light">με</syll><syll weight="heavy">νον</syll> <syll weight="heavy" anceps="True">τοῖς</syll> <syll weight="light" resolution="True">προ</syll><syll weight="light" resolution="True">βα</syll><syll weight="light">τί</syll><syll weight="heavy">οις</syll>, </l>
          <l n="300" metre="2 ia"><syll weight="heavy" anceps="True">εἰ</syll><syll weight="heavy">κῇ</syll> <syll weight="light">δὲ</syll> <syll weight="light" resolution="True">κα</syll><syll weight="light" resolution="True">τα</syll><syll weight="heavy" anceps="True">δαρ</syll><syll weight="heavy">θόν</syll><syll weight="light">τα</syll> <syll weight="heavy">που</syll> </l>
          <l n="301" metre="4 ia^"><syll weight="light" anceps="True">μέ</syll><syll weight="heavy">γαν</syll> <syll weight="light">λα</syll><syll weight="heavy">βόν</syll><syll weight="light" anceps="True">τε</syll><syll weight="heavy">ς ἡμ</syll><syll weight="light">μέ</syll><syll weight="heavy">νον</syll> <syll weight="heavy" anceps="True">σφη</syll><syll weight="heavy">κίσ</syll><syll weight="light">κο</syll><syll weight="heavy">ν ἐκ</syll><syll weight="light">τυ</syll><syll weight="heavy">φλῶ</syll><syll weight="heavy">σαι</syll>. </l>
        </strophe>
      </canticum>
      <canticum>
        <strophe type="strophe" responsion="pl02">
          <l n="303" metre="4 ia^"><syll weight="heavy" anceps="True">ἣ</syll> <syll weight="heavy">τοὺ</syll><syll weight="light">ς ἑ</syll><syll weight="heavy">ταί</syll><syll weight="heavy" anceps="True">ρους</syll> <syll weight="heavy">τοῦ</syll>—<syll weight="light">Φι</syll><syll weight="heavy">λω</syll><syll weight="light" anceps="True">νί</syll><syll weight="heavy">δου</syll> <syll weight="light">πο</syll><syll weight="heavy">τ' ἐν</syll> <syll weight="light">Κο</syll><syll weight="heavy">ρίν</syll><syll weight="heavy">θῳ</syll> </l>
          <l n="304" metre="2 ia"><syll weight="light" anceps="True">ἔ</syll><syll weight="heavy">πει</syll><syll weight="light">σε</syll><syll weight="heavy">ν ὡ</syll><syll weight="heavy" anceps="True">ς ὄν</syll><syll weight="heavy">τας</syll> <syll weight="light">κά</syll><syll weight="heavy">πρους</syll> </l>
          <l n="305" metre="4 ia^"><syll weight="light" anceps="True">με</syll><syll weight="heavy">μαγ</syll><syll weight="light">μέ</syll><syll weight="heavy">νον</syll> <syll weight="heavy" anceps="True">σκῶ</syll><syll weight="heavy">ρ ἐσ</syll><syll weight="light">θί</syll><syll weight="heavy">ει</syll><syll weight="heavy">ν, αὐ</syll><syll weight="heavy">τὴ</syll> <syll weight="light">δ' ἔ</syll><syll weight="heavy">ματ</syll><syll weight="light">τε</syll><syll weight="heavy">ν αὐ</syll><syll weight="heavy">τοῖς</syll>, </l>
          <l n="306" metre="2 ia"><syll weight="heavy" anceps="True">μι</syll><syll weight="heavy">μή</syll><syll weight="light">σο</syll><syll weight="heavy">μαι</syll> <syll weight="heavy" anceps="True">πάν</syll><syll weight="heavy">τας</syll> <syll weight="light">τρό</syll><syll weight="heavy">πους</syll>· </l>
          <l n="307" metre="3 ia"><syll weight="heavy" anceps="True">ὑ</syll><syll weight="heavy">μεῖς</syll> <syll weight="light">δὲ</syll> <syll weight="heavy">γρυ</syll><syll weight="heavy" anceps="True">λίζ</syll><syll weight="heavy">ον</syll><syll weight="light">τε</syll><syll weight="light" resolution="True">ς ὑ</syll><syll weight="light" resolution="True">πὸ</syll> <syll weight="light" anceps="True">φι</syll><syll weight="heavy">λη</syll><syll weight="light">δί</syll><syll weight="heavy">ας</syll> </l>
        </strophe>
        <strophe type="antistrophe" responsion="pl02">
          <l n="310" metre="4 ia^"><syll weight="heavy" anceps="True">καὶ</syll> <syll weight="heavy">μαγ</syll><syll weight="light">γα</syll><syll weight="heavy">νεύ</syll><syll weight="heavy" anceps="True">ου</syll><syll weight="heavy">σαν</syll> <syll weight="light">μο</syll><syll weight="heavy">λύ</syll><syll weight="heavy" anceps="True">νου</syll><syll weight="heavy">σάν</syll> <syll weight="light">τε</syll> <syll weight="heavy">τοὺ</syll><syll weight="light">ς ἑ</syll><syll weight="heavy">ταί</syll><syll weight="heavy">ρους</syll> </l>
          <l n="311" metre="2 ia"><syll weight="light" anceps="True">λα</syll><syll weight="heavy">βόν</syll><syll weight="light">τε</syll><syll weight="light" resolution="True">ς ὑ</syll><syll weight="light" resolution="True">πὸ</syll> <syll weight="light" anceps="True">φι</syll><syll weight="heavy">λη</syll><syll weight="light">δί</syll><syll weight="heavy">ας</syll> </l>
          <l n="312" metre="4 ia^"><syll weight="heavy" anceps="True">τὸν</syll> <syll weight="heavy">Λαρ</syll><syll weight="light">τί</syll><syll weight="heavy">ου</syll> <syll weight="heavy" anceps="True">μι</syll><syll weight="heavy">μού</syll><syll weight="light">με</syll><syll weight="heavy">νοι</syll> <syll weight="heavy" anceps="True">τῶ</syll><syll weight="heavy">ν ὄρ</syll><syll weight="light">χε</syll><syll weight="heavy">ων</syll> <syll weight="light">κρε</syll><syll weight="heavy">μῶ</syll><syll weight="heavy">μεν</syll>, </l>
          <l n="313" metre="2 ia"><syll weight="heavy" anceps="True">μιν</syll><syll weight="heavy">θώ</syll><syll weight="light">σο</syll><syll weight="heavy">μέν</syll> <syll weight="heavy" anceps="True">θ' ὥσ</syll><syll weight="heavy">περ</syll> <syll weight="light">τρά</syll><syll weight="heavy">γου</syll> </l>
          <l n="314" metre="3 ia"><syll weight="heavy" anceps="True">τὴν</syll> <syll weight="heavy">ῥῖ</syll><syll weight="light">να</syll>· <syll weight="light" resolution="True">σὺ</syll> <syll weight="light" resolution="True">δ' Ἀ</syll><syll weight="heavy" anceps="True">ρίσ</syll><syll weight="heavy">τυλ</syll><syll weight="light">λο</syll><syll weight="light" resolution="True">ς ὑ</syll><syll weight="light" resolution="True">πο</syll><syll weight="heavy" anceps="True">χάσ</syll><syll weight="heavy">κω</syll><syll weight="light">ν ἐ</syll><syll weight="heavy">ρεῖς</syll>· </l>
        </strophe>
      </canticum>
    </body>
