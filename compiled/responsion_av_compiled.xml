  <text>
    <body>
      <canticum genre="anapaestic, dochmic, cretic">
        <strophe type="strophe" responsion="av01">
          <l n="327" metre="ia" speaker="ΧΟ."><syll weight="light">Ἔ</syll><syll weight="heavy">α</syll> <syll weight="light">ἔ</syll><syll weight="heavy">α</syll>· </l>
          <l n="328" metre="3 an" brevis_in_longo="False"><syll weight="light">προ</syll><syll weight="light">δε</syll><syll weight="light">δό</syll><syll weight="light">με</syll><syll weight="light">θ' ἀ</syll><syll weight="light">νό</syll><syll weight="light">σι</syll><syll weight="light">ά</syll> <syll weight="light">τ' ἐ</syll><syll weight="light">πά</syll><syll weight="light">θο</syll><syll weight="light" brevis_in_longo="True">μεν</syll>· </l>
          <l n="329" metre="2 an"><syll weight="heavy">ὃς</syll> <syll weight="heavy">γὰρ</syll> <syll weight="light">φί</syll><syll weight="light">λο</syll><syll weight="heavy">ς ἦ</syll><syll weight="light">ν ὁ</syll><syll weight="light">μό</syll><syll weight="light">τρο</syll><syll weight="light">φά</syll> <syll weight="heavy">θ' ἡ</syll><syll weight="heavy">μῖν</syll> </l>
          <l n="330" metre="3 an"><syll weight="light">ἐ</syll><syll weight="light">νέ</syll><syll weight="light">με</syll><syll weight="light">το</syll> <syll weight="light">πε</syll><syll weight="light">δί</syll><syll weight="light">α</syll> <syll weight="light">πα</syll><syll weight="heavy">ρ' ἡ</syll><syll weight="heavy">μῖν</syll>, </l>
          <l n="331" metre="2 an"><syll weight="light" resolution="True">πα</syll><syll weight="light" resolution="True">ρέ</syll><syll weight="heavy">βη</syll> <syll weight="heavy">μὲν</syll> <syll weight="heavy">θεσ</syll><syll weight="heavy">μοὺ</syll><syll weight="heavy">ς ἀρ</syll><syll weight="heavy">χαί</syll><syll weight="heavy">ους</syll>, </l>
          <l n="332" metre="2 an^"><syll weight="light" resolution="True">πα</syll><syll weight="light" resolution="True">ρέ</syll><syll weight="heavy">βη</syll> <syll weight="heavy">δ' ὅρ</syll><syll weight="heavy">κου</syll><syll weight="heavy">ς ὀρ</syll><syll weight="heavy">νί</syll><syll weight="heavy">θων</syll>. </l>
        </strophe>
        <strophe type="antistrophe" responsion="av01">
          <l n="343" metre="ia" speaker="ΧΟ."><syll weight="light">Ἰ</syll><syll weight="heavy">ὼ</syll> <syll weight="light">ἰ</syll><syll weight="heavy">ώ</syll>· </l>
          <l n="344" metre="3 an"><syll weight="light">ἔ</syll><syll weight="light">πα</syll><syll weight="light">γ' ἔ</syll><syll weight="light">πι</syll><syll weight="light">θ' ἐ</syll><syll weight="light">πί</syll><syll weight="light">φε</syll><syll weight="light">ρε</syll> <syll weight="light">πο</syll><syll weight="light">λέ</syll><syll weight="light">μι</syll><syll weight="light" brevis_in_longo="True">ον</syll> </l>
          <l n="345" metre="2 an"><syll weight="heavy">ὁρ</syll><syll weight="heavy">μὰν</syll> <syll weight="light">φο</syll><syll weight="light">νί</syll><syll weight="heavy">αν</syll>, <syll weight="light">πτέ</syll><syll weight="light">ρυ</syll><syll weight="light">γά</syll> <syll weight="light">τε</syll> <syll weight="heavy">παν</syll><syll weight="heavy">τᾷ</syll> </l>
          <l n="346" metre="3 an"><syll weight="light">πε</syll><syll weight="light">ρί</syll><syll weight="light">βα</syll><syll weight="light">λε</syll> <syll weight="light">πε</syll><syll weight="light">ρί</syll> <syll weight="light">τε</syll> <syll weight="light">κύ</syll><syll weight="heavy">κλω</syll><syll weight="heavy">σαι</syll>· </l>
          <l n="347" metre="2 an"><syll weight="heavy">ὡς</syll> <syll weight="heavy">δεῖ</syll> <syll weight="heavy">τώ</syll><syll weight="heavy">δ' οἰ</syll><syll weight="heavy">μώζ</syll><syll weight="heavy">ει</syll><syll weight="heavy">ν ἄμ</syll><syll weight="heavy">φω</syll> </l>
          <l n="348" metre="2 an^"><syll weight="heavy">καὶ</syll> <syll weight="heavy">δοῦ</syll><syll weight="heavy">ναι</syll> <syll weight="heavy">ῥύγ</syll><syll weight="heavy">χει</syll> <syll weight="heavy">φορ</syll><syll weight="heavy">βάν</syll>. </l>
        </strophe>
      </canticum>
      <canticum genre="undeveloped dactylo-epitrite">
        <strophe type="strophe" responsion="av02">
          <l n="451" metre="" speaker="ΧΟ."><syll weight="light">Δο</syll><syll weight="light">λε</syll><syll weight="heavy">ρὸν</syll> <syll weight="light">μὲ</syll><syll weight="light">ν ἀ</syll><syll weight="heavy">εὶ</syll> <syll weight="light">κα</syll><syll weight="light">τὰ</syll> <syll weight="heavy">πάν</syll><syll weight="light">τα</syll> <syll weight="heavy">δὴ</syll> <syll weight="light">τρό</syll><syll weight="heavy">πον</syll> </l>
          <l n="452" metre=""><syll weight="light" anceps="True">πέ</syll><syll weight="heavy">φυ</syll><syll weight="light">κε</syll><syll weight="heavy">ν ἄν</syll><syll weight="heavy">θρω</syll><syll weight="heavy">πος</syll>· <syll weight="light">σὺ</syll> <syll weight="light">δ' ὅ</syll><syll weight="heavy">μως</syll> <syll weight="light">λέ</syll><syll weight="light">γε</syll> <syll weight="heavy">μοι</syll>. <syll weight="light">τά</syll><syll weight="light">χα</syll> <syll weight="heavy">γὰρ</syll> </l>
          <l n="453-455" metre=""><syll weight="light">τύ</syll><syll weight="heavy">χοι</syll><syll weight="heavy">ς ἂν</syll> <syll weight="heavy">χρησ</syll><syll weight="light">τὸ</syll><syll weight="heavy">ν ἐξ</syll><syll weight="heavy">ει</syll><syll weight="heavy">πὼ</syll><syll weight="light">ν ὅ</syll> <syll weight="light">τι</syll> <syll weight="heavy">μοι</syll> <syll weight="light">πα</syll><syll weight="light">ρο</syll><syll weight="heavy">ρᾷ</syll><syll weight="heavy">ς, ἢ</syll> <syll weight="light">δύ</syll><syll weight="light">να</syll><syll weight="heavy">μίν</syll> <syll weight="light">τι</syll><syll weight="light">να</syll> <syll weight="heavy">μείζ</syll><syll weight="heavy">ω</syll> </l>
          <l n="456" metre=""><syll weight="light">πα</syll><syll weight="light">ρα</syll><syll weight="heavy">λει</syll><syll weight="light">πο</syll><syll weight="light">μέ</syll><syll weight="heavy">νη</syll><syll weight="light">ν ὑ</syll><syll weight="light">π' ἐ</syll><syll weight="heavy">μῆς</syll> <syll weight="light">φρε</syll><syll weight="light">νὸ</syll><syll weight="heavy">ς ἀξ</syll><syll weight="light">υ</syll><syll weight="light">νέ</syll><syll weight="heavy">του</syll>· </l>
          <l n="457" metre=""><syll weight="light">σὺ</syll> <syll weight="light">δὲ</syll> <syll weight="heavy">τοῦ</syll><syll weight="heavy">θ' οὑ</syll><syll weight="heavy">ρᾷς</syll> <syll weight="light">λέ</syll><syll weight="heavy">γ' εἰς</syll> <syll weight="heavy">κοι</syll><syll weight="light" brevis_in_longo="True">νόν</syll>. </l>
          <l n="458" metre=""><syll weight="light">Ὃ</syll> <syll weight="light">γὰ</syll><syll weight="heavy">ρ ἂν</syll> <syll weight="light">σὺ</syll> <syll weight="light">τύ</syll><syll weight="heavy">χῃς</syll> <syll weight="heavy">μοι</syll> </l>
        </strophe>
        <strophe type="antistrophe" responsion="av02">
          <l n="539" metre="" speaker="ΧΟ."><syll weight="light">Πο</syll><syll weight="light">λὺ</syll> <syll weight="heavy">δὴ</syll> <syll weight="light">πο</syll><syll weight="light">λὺ</syll> <syll weight="heavy">δὴ</syll> <syll weight="light">χα</syll><syll weight="light">λε</syll><syll weight="heavy">πω</syll><syll weight="light">τά</syll><syll weight="heavy">τους</syll> <syll weight="light">λό</syll><syll weight="heavy">γους</syll> </l>
          <l n="540" metre=""><syll weight="heavy" anceps="True">ἤ</syll><syll weight="heavy">νεγ</syll><syll weight="light">κα</syll><syll weight="heavy">ς, ἄν</syll><syll weight="heavy">θρω</syll><syll weight="heavy">φ'· ὡ</syll><syll weight="light">ς ἐ</syll><syll weight="light">δά</syll><syll weight="heavy">κρυ</syll><syll weight="light">σά</syll> <syll weight="light">γ' ἐ</syll><syll weight="heavy">μῶν</syll> <syll weight="light">πα</syll><syll weight="light">τέ</syll><syll weight="heavy">ρων</syll> </l>
          <l n="541-543" metre=""><syll weight="light">κά</syll><syll weight="heavy">κη</syll><syll weight="heavy">ν, οἳ</syll> <syll weight="heavy">τάσ</syll><syll weight="light">δε</syll> <syll weight="heavy">τὰς</syll> <syll weight="heavy">τι</syll><syll weight="heavy">μὰς</syll> <syll weight="light">προ</syll><syll weight="light">γό</syll><syll weight="heavy">νων</syll> <syll weight="light">πα</syll><syll weight="light">ρα</syll><syll weight="heavy">δόν</syll><syll weight="heavy">τω</syll><syll weight="light">ν ἐ</syll><syll weight="light">π' ἐ</syll><syll weight="heavy">μοὶ</syll> <syll weight="light">κα</syll><syll weight="light">τέ</syll><syll weight="heavy">λυ</syll><syll weight="heavy">σαν</syll>. </l>
          <l n="544" metre=""><syll weight="light">Σὺ</syll> <syll weight="light">δέ</syll> <syll weight="heavy">μοι</syll> <syll weight="light">κα</syll><syll weight="light">τὰ</syll> <syll weight="heavy">δαί</syll><syll weight="light">μο</syll><syll weight="light">να</syll> <syll weight="heavy">καί</syll> <syll weight="light">τι</syll><syll weight="light">να</syll> <syll weight="heavy">συν</syll> <syll weight="light">τυ</syll><syll weight="light">χί</syll><syll weight="heavy">αν</syll> </l>
          <l n="545" metre=""><syll weight="light">ἀ</syll><syll weight="light">γα</syll><syll weight="heavy">θὴν</syll> <syll weight="heavy">ἥ</syll><syll weight="heavy">κει</syll><syll weight="light">ς ἐ</syll><syll weight="heavy">μοὶ</syll> <syll weight="heavy">σω</syll><syll weight="heavy">τήρ</syll>. </l>
          <l n="546" metre=""><syll weight="light">Ἀ</syll><syll weight="light">να</syll><syll weight="heavy">θεὶς</syll> <syll weight="light">γὰ</syll><syll weight="light">ρ ἐ</syll><syll weight="heavy">γώ</syll> <syll weight="heavy">σοι</syll> </l>
