  <text>
    <body>
      <canticum>
        <strophe type="strophe" responsion="baseline01">
          <l n="1" metre="" speaker="ΔΙΚΑΙΟΠΟΛΙΣ"><syll weight="light" resolution="True">Ὅ</syll><syll weight="light" resolution="True">σα</syll> <syll weight="heavy">δὴ</syll> <syll weight="light">δέ</syll><syll weight="heavy">δηγ</syll><syll weight="heavy" anceps="True">μαι</syll> <syll weight="heavy">τὴ</syll><syll weight="light">ν ἐ</syll><syll weight="heavy">μαυ</syll><syll weight="heavy" anceps="True">τοῦ</syll> <syll weight="heavy">καρ</syll><syll weight="light">δί</syll><syll weight="heavy">αν</syll>, </l>
          <l n="2" metre=""><syll weight="heavy" anceps="True">ἥσ</syll><syll weight="heavy">θην</syll> <syll weight="light">δὲ</syll> <syll weight="heavy">βαι</syll><syll weight="light" anceps="True">ά</syll>, <syll weight="light" resolution="True">πά</syll><syll weight="light" resolution="True">νυ</syll> <syll weight="light">δὲ</syll> <syll weight="heavy">βαι</syll><syll weight="light" anceps="True">ά</syll>, <syll weight="heavy">τέτ</syll><syll weight="light">τα</syll><syll weight="light" brevis_in_longo="True">ρα</syll>· </l>
          <l n="3" metre=""><syll weight="light" anceps="True">ἃ</syll> <syll weight="heavy">δ' ὠ</syll><syll weight="light">δυ</syll><syll weight="heavy">νή</syll><syll weight="heavy" anceps="True">θην</syll>, <syll weight="heavy">ψαμ</syll><syll weight="light">μα</syll><syll weight="light" resolution="True">κο</syll><syll weight="light" resolution="True">σι</syll><syll weight="light" anceps="True">ο</syll><syll weight="heavy">γάρ</syll><syll weight="light">γα</syll><syll weight="light" brevis_in_longo="True">ρα</syll>. </l>
          <l n="4" metre=""><syll weight="light" resolution="True">Φέ</syll><syll weight="light" resolution="True">ρ' ἴ</syll><syll weight="heavy">δω</syll>, <syll weight="light">τί</syll> <syll weight="heavy">δ' ἥσ</syll><syll weight="heavy" anceps="True">θη</syll><syll weight="heavy">ν ἄξ</syll><syll weight="light">ι</syll><syll weight="heavy">ον</syll> <syll weight="heavy" anceps="True">χαι</syll><syll weight="heavy">ρη</syll><syll weight="light">δό</syll><syll weight="heavy">νος</syll>; </l>
          <l n="5" metre=""><syll weight="light" anceps="True">Ἐ</syll><syll weight="heavy">γᾦ</syll><syll weight="light">δ' ἐ</syll><syll weight="heavy">φ' ᾧ</syll> <syll weight="light" anceps="True">γε</syll> <syll weight="light" resolution="True">τὸ</syll> <syll weight="light" resolution="True">κέ</syll><syll weight="light">α</syll><syll weight="heavy">ρ εὐ</syll><syll weight="heavy" anceps="True">φράν</syll><syll weight="heavy">θη</syll><syll weight="light">ν ἰ</syll><syll weight="heavy">δών</syll>, </l>
          <l n="6" metre=""><syll weight="heavy" anceps="True">ἀλ</syll><syll weight="heavy">λ' αὐ</syll><syll weight="light">τὸ</syll><syll weight="light" resolution="True">ς ἔ</syll><syll weight="light" resolution="True">φε</syll><syll weight="light" anceps="True">ρε</syll> <syll weight="heavy">πάν</syll><syll weight="light">τα</syll> <syll weight="heavy">χὠ</syll> <syll weight="light" anceps="True">πρί</syll><syll weight="heavy">ω</syll><syll weight="light">ν ἀ</syll><syll weight="heavy">πῆν</syll>. </l>
          <l n="7" metre=""><syll weight="heavy" anceps="True">Νῦ</syll><syll weight="heavy">ν οὖ</syll><syll weight="light">ν ἀ</syll><syll weight="light">τεχ</syll><syll weight="heavy">νῶ</syll><syll weight="heavy" anceps="True">ς ἥ</syll><syll weight="heavy">κω</syll> <syll weight="light">πα</syll><syll weight="heavy">ρεσ</syll><syll weight="heavy" anceps="True">κευ</syll><syll weight="heavy">ασ</syll><syll weight="light">μέ</syll><syll weight="heavy">νος</syll> </l>
          <l n="8" metre=""><syll weight="light" resolution="True">δι</syll><syll weight="light" resolution="True">ὰ</syll> <syll weight="heavy">τοῦ</syll><syll weight="light">το</syll> <syll weight="heavy">τοὔρ</syll><syll weight="light" anceps="True">γο</syll><syll weight="heavy">ν· ἄξ</syll><syll weight="light">ι</syll><syll weight="heavy">ον</syll> <syll weight="light" anceps="True">γὰ</syll><syll weight="heavy">ρ Ἑλ</syll><syll weight="light">λά</syll><syll weight="light" brevis_in_longo="True">δι</syll>. </l>
        </strophe>
        <strophe type="antistrophe" responsion="baseline01">
          <l n="9" metre=""><syll weight="heavy" anceps="True">Ἀλ</syll><syll weight="heavy">λ' ὠ</syll><syll weight="light">δυ</syll><syll weight="heavy">νή</syll><syll weight="heavy" anceps="True">θη</syll><syll weight="light" resolution="True">ν ἕ</syll><syll weight="light" resolution="True">τε</syll><syll weight="light">ρο</syll><syll weight="heavy">ν αὖ</syll> <syll weight="light">τρα</syll><syll weight="heavy">γῳ</syll><syll weight="light">δι</syll><syll weight="heavy">κόν</syll>, </l>
          <l n="10" metre=""><syll weight="light" resolution="True">ὅ</syll><syll weight="light" resolution="True">τε</syll> <syll weight="heavy">δὴ</syll> '<syll weight="light">κε</syll><syll weight="heavy">χή</syll><syll weight="heavy" anceps="True">νη</syll> <syll weight="heavy">προσ</syll><syll weight="light">δο</syll><syll weight="heavy">κῶν</syll> <syll weight="light" anceps="True">τὸ</syll><syll weight="heavy">ν Αἰ</syll><syll weight="light">σχύ</syll><syll weight="heavy">λον</syll>, </l>
          <l n="11" metre=""><syll weight="light" resolution="True">ὁ</syll> <syll weight="light" resolution="True">δ' ἀ</syll><syll weight="heavy">νεῖ</syll><syll weight="light">πε</syll><syll weight="heavy">ν· Εἴ</syll><syll weight="light" anceps="True">σα</syll><syll weight="heavy">γ', ὦ</syll> <syll weight="light">Θέ</syll><syll weight="heavy">ογ</syll><syll weight="light" anceps="True">νι</syll>, <syll weight="heavy">τὸν</syll> <syll weight="light">χο</syll><syll weight="heavy">ρόν</syll>. </l>
          <l n="12" metre=""><syll weight="heavy" anceps="True">Πῶς</syll> <syll weight="heavy">τοῦ</syll><syll weight="light">τ' ἔ</syll><syll weight="heavy">σει</syll><syll weight="light" anceps="True">σέ</syll> <syll weight="heavy">μου</syll> <syll weight="light">δο</syll><syll weight="heavy">κεῖς</syll> <syll weight="heavy" anceps="True">τὴν</syll> <syll weight="heavy">καρ</syll><syll weight="light">δί</syll><syll weight="heavy">αν</syll>; </l>
          <l n="13" metre=""><syll weight="heavy" anceps="True">Ἀλ</syll><syll weight="light" resolution="True">λ' ἕ</syll><syll weight="light" resolution="True">τε</syll><syll weight="light">ρο</syll><syll weight="heavy">ν ἥσ</syll><syll weight="heavy" anceps="True">θη</syll><syll weight="heavy">ν, ἡ</syll><syll weight="light">νί</syll><syll weight="light" resolution="True">κ' ἐ</syll><syll weight="light" resolution="True">πὶ</syll> <syll weight="heavy" anceps="True">Μόσ</syll><syll weight="heavy">χῳ</syll> <syll weight="light">πο</syll><syll weight="light" brevis_in_longo="True">τὲ</syll> </l>
          <l n="14" metre=""><syll weight="heavy" anceps="True">Δεξ</syll><syll weight="light" resolution="True">ί</syll><syll weight="light" resolution="True">θε</syll><syll weight="light">ο</syll><syll weight="heavy">ς εἰ</syll><syll weight="heavy" anceps="True">σῆλ</syll><syll weight="heavy">θ' ᾀ</syll><syll weight="light">σό</syll><syll weight="light" resolution="True">με</syll><syll weight="light" resolution="True">νος</syll> <syll weight="heavy" anceps="True">Βοι</syll><syll weight="heavy">ώ</syll><syll weight="light">τι</syll><syll weight="light" brevis_in_longo="True">ον</syll>. </l>
          <l n="15" metre=""><syll weight="heavy" anceps="True">τοῖς</syll> <syll weight="heavy">πέν</syll><syll weight="light">τε</syll> <syll weight="light">τα</syll><syll weight="heavy">λάν</syll><syll weight="heavy" anceps="True">τοι</syll><syll weight="heavy">ς οἷς</syll> <syll weight="light">Κλέ</syll><syll weight="heavy">ω</syll><syll weight="heavy" anceps="True">ν ἐξ</syll><syll weight="heavy">ή</syll><syll weight="light">με</syll><syll weight="heavy">σεν</syll>. </l>
          <l n="16" metre=""><syll weight="heavy" anceps="True">Τῆ</syll><syll weight="heavy">τες</syll> <syll weight="light">δ' ἀ</syll><syll weight="light" resolution="True">πέ</syll><syll weight="light" resolution="True">θα</syll><syll weight="heavy" anceps="True">νον</syll> <syll weight="heavy">καὶ</syll> <syll weight="light">δι</syll><syll weight="heavy">εσ</syll><syll weight="light" anceps="True">τρά</syll><syll weight="heavy">φη</syll><syll weight="light">ν ἰ</syll><syll weight="heavy">δών</syll>, </l>
        </strophe>
      </canticum>
    </body>
  </text>
